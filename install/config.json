{
  "dockerImageBuilder": "cimg/base:stable-20.04",
  "inputs": [
    {
      "label": "Install Homebrew and its packages? ",
      "name": "HOMEBREW",
      "type": "bool",
      "items": [
        "true",
        "false"
      ],
      "default": "true"
    },
    {
      "label": "Install Powerlevel10k terminal? ",
      "name": "TERMINAL",
      "type": "bool",
      "items": [
        "true",
        "false"
      ],
      "default": "true"
    },
  ]
}
