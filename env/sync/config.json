{
  "dockerImageBuilder": "cimg/node:14.0",
  "inputs": [
    {
      "name": "gist_token",
      "type": "CREDENTIAL_GITHUB_TOKEN"
    },
    
    {
      "default": "~/.zshrc",
      "items": [
        "~/.zshrc",
        "~/.p10k.zsh"
      ],
      "label": "Pick file you want to sync: ",
      "name": "file",
      "type": "text"
    }
  ]
}
